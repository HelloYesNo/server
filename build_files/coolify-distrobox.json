{
  "distrobox_version": "1.7.2",
  "name": "coolify",
  "image": "registry.fedoraproject.org/fedora:latest",
  "compatibility": ["podman", "docker"],
  "root": false,
  "start_now": false,
  "init": false,
  "nvidia": false,
  "unshare_ipc": false,
  "unshare_netns": false,
  "unshare_process": false,
  "unshare_devsys": false,
  "unshare_all": false,
  "custom_command": "",
  "additional_flags": [
    "--volume=/var/run/docker.sock:/var/run/docker.sock",
    "--publish=8000:8000",
    "--volume=/var/lib/coolify:/var/lib/coolify:Z",
    "--env=COOLIFY_DATA_DIR=/var/lib/coolify",
    "--env=DOCKER_HOST=unix:///var/run/docker.sock"
  ],
  "init_hook": "dnf install -y docker docker-compose curl wget git openssl jq && systemctl enable docker --now || true",
  "pre_init_hook": "",
  "post_init_hook": "echo 'Coolify distrobox container initialized'",
  "pre_start_hook": "",
  "post_start_hook": "",
  "pre_stop_hook": "",
  "post_stop_hook": "",
  "pre_rm_hook": "",
  "post_rm_hook": "",
  "entrypoint": "/bin/bash",
  "startup_time": 10,
  "hostname": "coolify-container",
  "username": "coolify",
  "user_uid": 9999,
  "user_gid": 0,
  "user_home": "/var/lib/coolify",
  "skip_workdir": false,
  "workdir": "/var/lib/coolify",
  "description": "Coolify container for Universal Blue - manages Docker containers on host"
}